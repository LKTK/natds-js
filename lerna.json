{
  "command": {
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": [
        "packages/icons/**"
      ],
      "message": "chore(release): publish %s via lerna [skip ci]"
    }
  },
  "lerna": "3.16.4",
  "packages": [
    "examples/*",
    "examples/single-spa/*",
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "1.0.0-alpha.2"
}
